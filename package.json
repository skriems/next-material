    "compression": "^1.7.2",
    "express": "^4.16.3",
    "jss": "^9.8.7",
    "next": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-jss": "^8.6.1",
    "serverless-http": "^2.2.0"
  },
  "devDependencies": {
    "serverless-apigw-binary": "^0.4.4",
    "serverless-domain-manager": "^3.0.1",
    "serverless-plugin-warmup": "^3.6.1-rc.1"
  }
}
