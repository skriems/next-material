{
  "name": "next-material",
  "version": "0.0.1",
  "description": "Next.js on AWS Lambda leveraging serverless-http and material-ui",
  "author": "Sebastian Kriems",
  "repository": {
    "type": "git",
    "url": "https://github.com/skriems/next-material"
  },
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "dev:server": "npm run build && node server.js",
